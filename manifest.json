{
  "manifest_version": 3,
  "name": "Leetcode Submit Prevention",
  "version": "0.15",
  "description": "Prevent accidental submissions by requiring a double press in leetcode v.0.14",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },"action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts": [{
    "matches": ["*://leetcode.com/problems/*"],
    "js": ["content.js"]
  }]
}